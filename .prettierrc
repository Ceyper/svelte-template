{
  "useTabs": false,
  "tabWidth": 4,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "es5",
  "printWidth": 100,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "quoteProps": "as-needed",
  "endOfLine": "lf",
  "overrides": [
    {
      "files": [
        "*.json",
        "*.html"
      ],
      "options": {
        "tabWidth": 2
      }
    }
  ],
  "plugins": [
    "prettier-plugin-svelte"
  ]
}
